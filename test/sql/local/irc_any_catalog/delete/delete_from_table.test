# name: test/sql/local/irc_any_catalog/delete/delete_from_table.test
# description: test integration with iceberg catalog read
# group: [delete]

require-env ICEBERG_SERVER_AVAILABLE

require-env SECRETS_CREATED_AND_CATALOG_ATTACHED

require avro

require parquet

require iceberg

require httpfs

# Do not ignore 'HTTP' error messages!
set ignore_error_messages

statement ok
CALL enable_logging('HTTP');

statement ok
set logging_level='debug'

statement ok
drop table if exists my_datalake.default.test_delete_table;

statement ok
create table my_datalake.default.test_delete_table (a int);

loop i 0 5

statement ok
insert into my_datalake.default.test_delete_table values ((${i}*2)), ((${i}*2) + 1);

endloop

statement ok
delete from my_datalake.default.test_delete_table where a%2 = 0;

query I
select * from my_datalake.default.test_delete_table order by all;
----
1
3
5
7
9

statement ok
drop table my_datalake.default.test_delete_table;
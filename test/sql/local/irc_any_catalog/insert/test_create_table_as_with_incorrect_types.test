# name: test/sql/local/irc_any_catalog/insert/test_create_table_as_with_incorrect_types.test
# description: test integration with iceberg catalog read
# group: [insert]

require-env ICEBERG_SERVER_AVAILABLE

require-env SECRETS_CREATED_AND_CATALOG_ATTACHED

require avro

require parquet

require iceberg

require httpfs

# Do not ignore 'HTTP' error messages!
set ignore_error_messages

statement ok
CALL enable_logging('HTTP');

statement ok
set logging_level='debug'


statement ok
create schema if not exists my_datalake.new_schema_1;

statement ok

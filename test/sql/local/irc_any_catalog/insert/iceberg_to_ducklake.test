# name: test/sql/local/irc_any_catalog/insert/iceberg_to_ducklake.test
# group: [insert]

require-env ICEBERG_SERVER_AVAILABLE

require-env SECRETS_CREATED_AND_CATALOG_ATTACHED

require avro

require parquet

require iceberg

require httpfs

require ducklake

# Do not ignore 'HTTP' error messages!
set ignore_error_messages

mode skip

statement ok
set enable_logging=true

statement ok
set logging_level='debug'

statement ok
ATTACH 'ducklake:duckdb:__TEST_DIR__/ducklake.duckdb' as my_ducklake (DATA_PATH '__TEST_DIR__/data_path');

statement ok
call iceberg_to_ducklake(
    'my_datalake',
    'my_ducklake',
    skip_tables := [
        'pyspark_iceberg_table_v2',
        'deletion_vectors'
    ]
)

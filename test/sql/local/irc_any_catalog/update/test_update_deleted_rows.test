# name: test/sql/local/irc_any_catalog/update/test_update_deleted_rows.test
# description: Test iceberg deletes
# group: [update]

require-env ICEBERG_SERVER_AVAILABLE

require-env SECRETS_CREATED_AND_CATALOG_ATTACHED

require avro

require parquet

require iceberg

require httpfs

# Do not ignore 'HTTP' error messages!
set ignore_error_messages

statement ok
CALL enable_logging('HTTP');

statement ok
set logging_level='debug'

statement ok
create schema if not exists my_datalake.default;

statement ok
DROP TABLE IF EXISTS my_datalake.default.test;

statement ok
CREATE TABLE my_datalake.default.test AS SELECT range id FROM range(50000);

statement ok
begin;

statement ok
insert into my_datalake.default.test select range id from range(50000, 100000);

statement ok
delete from my_datalake.default.test where id % 10 = 0;

statement ok
Update my_datalake.default.test set id = 9999999999 where id % 10 = 0;

statement ok
commit;

query I
select count(*) from my_datalake.default.test where id = 9999999999;
----
0
